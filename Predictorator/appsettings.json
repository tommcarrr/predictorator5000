{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RateLimiting": {
    "ExcludedIPs": []
  },
  "GameWeekCache": {
    "CacheDurationHours": 2
