{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RateLimiting": {
    "ExcludedIPs": []
  },
  "GameWeekCache": {
    "CacheDurationHours": 2
  },
  "Hangfire": {
    "RunServer": true,
    "QueuePollIntervalSeconds": 120,
    "SchedulePollingIntervalSeconds": 120,
    "ServerCheckIntervalSeconds": 120,
    "HeartbeatIntervalSeconds": 120,
    "WorkerCount": 1
  },
  "AllowedHosts": "*"
}
